<%- include('header') %>
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">
                    <div class="row">
                        <div class="col-md-6 grid-margin stretch-card">
                            <div class="card">
                                <div class="card-body">
                                    <h4 class="card-title">profile</h4>
                                    <br>
                                    <img src="<%=profileData.profile%>" width="100" height="100" alt="">
                                    <br><br>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <address>
                                                <p class="font-weight-bold"><%=profileData.name%></p>
                                                <p>
                                                    <%=profileData.role%>
                                                </p>
                                                <p>
                                                    <%=profileData.gender%>
                                                </p>
                                                <p>
                                                    <%=profileData.hobby%>
                                                </p>
                                            </address>
                                        </div>
                                        <div class="col-md-6">
                                            <address class="text-primary">
                                                <p class="font-weight-bold">
                                                    E-mail
                                                </p>
                                                <p class="mb-2">
                                                    <%=profileData.email%>
                                                </p>
                                                <p class="font-weight-bold">
                                                    created date
                                                </p>
                                                <p>
                                                    <%=profileData.createdDate%>
                                                </p>
                                            </address>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <%- include('footer') %>