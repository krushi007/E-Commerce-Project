      function getSubCategory(){
        var catId = document.getElementById('categoryId').value

        $.ajax({
          url : "/extraCategory/getSubCategory",
          method : "post",
          data : {  
            'categoryId' : catId
          },
          success : function(res){
            $('#subCategoryId').html(res)
          }
        })
      }

      function getExtraCategory(){
        var subId = document.getElementById('subCategoryId').value
        $.ajax({
          url : "/brand/getExtraCategory",
          method : "post",
          data : {  
            'subCategoryId' : subId
          },
          success : function(res){
            $('#extraCategoryId').html(res)
          }
        })
      }

      function getBrandType(){
        var catId = document.getElementById('categoryId').value
        var subId = document.getElementById('subCategoryId').value
        var extraId = document.getElementById('extraCategoryId').value

        $.ajax({
          url : "/product/getBrandType",
          method : "post",
          data : {  
            'categoryId' : catId,
            'subCategoryId' : subId,
            'extraCategoryId' : extraId
          },
          success : function(res){
            $('#brandType').html(res)
          }
        })
      }

</script>

